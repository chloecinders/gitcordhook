{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": ["ES2020", "WebWorker"],
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"jsxImportSource": "@",
		"paths": {
			"@/jsx-runtime": ["./src/components/runtime"]
		}
	},
	"include": [
		"src",
		"worker-configuration.d.ts",
		"discord-types.d.ts",
		"src/components"
	],
	"exclude": ["node_modules", "dist"]
}
