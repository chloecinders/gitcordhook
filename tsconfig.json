{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": ["ES2020", "WebWorker"],
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": ["src", "worker-configuration.d.ts", "discord-types.d.ts"],
	"exclude": ["node_modules", "dist"]
}
